                $(window).on('wheel', function (e) {
                    var delta = e.originalEvent.deltaY;

                    if ($(window).scrollTop() + $(window).height() >= $(document).height() - 1) {
                        $('header').show();
                        $('footer').show();
                    }
                    else if (delta > 0) {
                        //down
                        $('header').fadeOut(500);
                        $('footer').fadeOut(500);
                    }
                    else {
                        //up
                        $('header').fadeIn(500);
                        $('footer').fadeIn(500);
                    }
                })
            });
